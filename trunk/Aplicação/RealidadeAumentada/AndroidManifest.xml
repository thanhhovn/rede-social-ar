<?xml version="1.0" encoding="UTF-8"?>
<manifest android:versionCode="1" android:versionName="1.0"
    package="br.com.realidadeAumentada" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="10"/>
						
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/><!--  GPS -->
    <uses-permission android:name="android.permission.ACCESS_COARCE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <uses-permission android:name="android.permission.INTERNET"/><!--  Acesso a Internet -->
    
	<uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    
    <uses-permission android:name="android.permission.USE_CREDENTIALS"/>
    <uses-permission android:name="android.permission.PERSISTENT_ACTIVITY"/>
    
    
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">
        
        <activity 
           	android:name=".ApresentacaoActivity" 
            android:screenOrientation="sensorPortait" 
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <activity android:label="@string/app_name"
            	  android:name=".LoginActivity" 
            	  android:screenOrientation="sensorPortait" 
            	  android:configChanges="orientation">
            <intent-filter>
                <action android:name="TELA_LOGIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="LOGIN" />
            </intent-filter>
        </activity>
        
        <activity android:name="ListaRelacionamentoActivity" 
            	  android:label="Tipo de Relacionamento">
	        <intent-filter>
                <action android:name="RELACIONAMENTO" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="LISTA" />
            </intent-filter>
        </activity>
        
        <activity android:name="ListaEscolaridadeActivity" 
            	  android:label="Nível Escolar">
	        <intent-filter>
                <action android:name="NIVEL_ESCOLAR" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="LISTA" />
            </intent-filter>
        </activity>
        
        <activity android:name="Cadastro_usuarioActivity" 
            	  android:label="Cadastro de Usuário">
	        <intent-filter>
                <action android:name="USUARIO" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="CADASTRO" />
            </intent-filter>
        </activity>
        
        <activity android:name="Perfil_usuarioActivity" 
            	  android:label="Cadastro do Perfil">
	        <intent-filter>
                <action android:name="USUARIO" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="PERFIL" />
            </intent-filter>
        </activity>
        
<!--         UseGPSActivity1 -->
        <activity android:name="WsdlClient"
            	  android:label="GPS">
	        <intent-filter>
                <action android:name="TESTE" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="GPS" />
            </intent-filter>
        </activity>
        
        <uses-library android:name="com.google.android.maps" />
    </application>
    
</manifest>
