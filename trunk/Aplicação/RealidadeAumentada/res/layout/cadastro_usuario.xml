<!--       Escolaridade -->

	   <TextView
          android:id="@+id/tv_escolaridadeUsuario"
          android:textColor="#1D3472"
          android:layout_width="fill_parent"
	      android:layout_height="fill_parent"
          android:gravity="left"
          android:textStyle="bold"
          android:text="Nível Escolar " />
	 
       <EditText
           android:id="@+id/et_escolaridadeUsuario"
     	   android:layout_width="fill_parent"
	       android:layout_height="fill_parent"
           android:maxLength="10">
<!--            <requestFocus /> -->
       </EditText>
       
       
<!--       Profissão -->
	   <TextView
          android:id="@+id/tv_profissaoUsuario"
          android:textColor="#1D3472"
          android:layout_width="fill_parent"
	      android:layout_height="fill_parent"
          android:gravity="left"
