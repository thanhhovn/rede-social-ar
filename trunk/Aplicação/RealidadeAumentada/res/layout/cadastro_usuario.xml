	       android:layout_height="fill_parent"
           android:maxLength="12"
           android:password="true" >
<!--            <requestFocus /> -->
	   </EditText>  
	   
<!-- Sexo -->
       <TextView
           android:id="@+id/tv_sexoUsuario"
           android:textColor="#1D3472"
           android:layout_width="fill_parent"
           android:layout_height="fill_parent"
           android:gravity="left"
           android:textStyle="bold"
           android:text="Sexo: " />	
	                        
			
      <LinearLayout
          android:id="@+id/layoutCadastroSexoUsuario"
          android:layout_width="110dip"
      	android:layout_height="match_parent"
      	android:gravity="center_vertical"
      	android:orientation="vertical">

          <RadioGroup
           android:id="@+id/rd_SexoUsuario"
           android:layout_width="wrap_content"
              android:layout_height="wrap_content"
           android:orientation="horizontal"
           android:layout_marginRight="15dip" >

           <RadioButton
               android:id="@+id/rd_UsuarioMasculino"
               android:layout_width="wrap_content"
              	android:layout_height="wrap_content"
               android:textSize="8dip"
               android:text="M"/>

           <RadioButton
               android:id="@+id/rd_Usuario_Feminino"
               android:layout_width="fill_parent"
              	android:layout_height="fill_parent"
               android:textSize="8dip"
               android:text="F" />
       </RadioGroup>
      </LinearLayout>
	       
<!--        Data de Nascimento -->
       <TextView
           android:id="@+id/tv_dataNascimentoUsuario"
           android:layout_width="fill_parent"
           android:layout_height="fill_parent"
           android:layout_weight="0.79"
           android:gravity="left"
           android:text="Data Nascimento: "
           android:textColor="#1D3472"
           android:textStyle="bold" />
	
       <DatePicker
           android:id="@+id/ed_dataNascimentoUsuario"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content" />

<!--       Endereço -->
	   <TextView
